{
  "version": 2,
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "nextjs",
  "functions": {
    "app/api/**/*.ts": {
      "maxDuration": 30
    }
  },
  "env": {
    "AI_BASE_URL": "@ai_base_url",
    "AI_API_KEY": "@ai_api_key", 
    "AI_MODEL": "@ai_model"
  }
}